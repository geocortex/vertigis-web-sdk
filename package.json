{
    "name": "@vertigis/web-sdk",
    "version": "0.1.0",
    "description": "The SDK for extending Geocortex Web",
    "engines": {
        "node": ">=12.13.1"
    },
    "files": [
        "bin",
        "config",
        "lib",
        "scripts",
        "template"
    ],
    "bin": {
        "vertigis-web-sdk": "bin/vertigis-web-sdk.js"
    },
    "scripts": {
        "build": "cd template && npm link \"../\" --no-package-lock && npm run build",
        "create": "cross-env SDK_LOCAL_DEV=true node bin/vertigis-web-sdk.js create",
        "start": "cd template && npm link \"../\" --no-package-lock && npm run start",
        "test": "echo \"No tests yet. All clear!\""
    },
    "author": "Ian Schmitz <ianschmitz@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "autoprefixer": "^9.7.6",
        "chalk": "^4.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-spawn": "^7.0.2",
        "css-loader": "^3.5.1",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "fork-ts-checker-webpack-plugin": "^4.1.2",
        "fs-extra": "^9.0.0",
        "html-webpack-plugin": "^4.0.4",
        "postcss-loader": "^3.0.0",
        "react-dev-utils": "^10.2.1",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-loader": "^6.2.2",
        "webpack": "^4.42.1",
        "webpack-dev-server": "^3.10.3"
    },
    "devDependencies": {
        "@geocortex/web": "^5.6.0",
        "@types/node": "^12.12.31",
        "cross-env": "^7.0.2",
        "husky": "^4.2.3",
        "prettier": "^2.0.2",
        "pretty-quick": "^2.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "semantic-release": "^17.0.4",
        "semver": "^7.1.3",
        "typescript": "^3.8.3"
    },
    "cldr-data-urls-filter": "(cldr-core|cldr-numbers-modern)",
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
